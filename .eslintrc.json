{
    "extends": "eslint:recommended",
    "globals": {
        "require": true,
        "process": true,
        "__dirname": true,
        "module": true,
        "console": true
    },
    "plugins": [
    ],
    "env": {
        "es6": true,
        "browser": false,
        "node": false
    },
    "rules": {
        "prefer-reflect": [
            1,
            {
                "exceptions": [
                ]
            }
        ],
        "wrap-regex": 0,
        "space-before-function-paren": [
            1,
            {
                "anonymous": "always",
                "named": "never",
                "asyncArrow": "ignore"
            }
        ],
        "sort-vars": [
            1,
            {
                "ignoreCase": false
            }
        ],
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "id-match": [
            1,
            "^[a-zA-Z0-9_]+$",
            {
                "onlyDeclarations": false,
                "properties": true
            }
        ],
        "quote-props": [
            1,
            "as-needed",
            {
                "keywords": true,
                "unnecessary": true,
                "numbers": true
            }
        ],
        "padded-blocks": [
            1,
            {
                "blocks": "never",
                "classes": "never",
                "switches": "never"
            }
        ],
        "operator-assignment": [
            1,
            "always"
        ],
        "one-var": 0,
        "object-curly-spacing": 0,
        "no-unneeded-ternary": 1,
        "no-underscore-dangle": [
            1,
            {
                "allow": [],
                "allowAfterThis": false,
                "allowAfterSuper": false
            }
        ],
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-lonely-if": 1,
        "no-array-constructor": 1,
        "no-continue": 1,
        "no-inline-comments": 0,
        "newline-after-var": [
            1,
            "always"
        ],
        "new-parens": 1,
        "new-cap": [
            1,
            {
                "newIsCap": true,
                "capIsNew": true,
                "properties": true
            }
        ],
        "max-nested-callbacks": [
            1,
            5
        ],
        "lines-around-comment": [
            1,
            {
                "beforeBlockComment": true,
                "afterBlockComment": false,
                "beforeLineComment": false,
                "afterLineComment": false,
                "allowBlockStart": false,
                "allowBlockEnd": false,
                "allowObjectStart": false,
                "allowObjectEnd": false,
                "allowArrayStart": false,
                "allowArrayEnd": false
            }
        ],
        "key-spacing": [
            1,
            {
                "beforeColon": false,
                "afterColon": true,
                "mode": "strict"
            }
        ],
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1,
                "VariableDeclarator": 1,
                "outerIIFEBody": 1,
                "MemberExpression": 1,
                "FunctionDeclaration": {
                    "body": 1,
                    "parameters": "first"
                },
                "FunctionExpression": {
                    "body": 1,
                    "parameters": "first"
                },
                "CallExpression": {
                    "arguments": 1
                },
                "ArrayExpression": 1,
                "ObjectExpression": 1
            }
        ],
        "id-length": [
            1,
            {
                "min": 3,
                "max": 34,
                "exceptions": [
                    "x",
                    "y",
                    "ii",
                    "id",
                    "_"
                ]
            }
        ],
        "func-style": [
            1,
            "declaration",
            {
                "allowArrowFunctions": true
            }
        ],
        "func-names": 1,
        "camelcase": [
            1,
            {
                "properties": "always"
            }
        ],
        "no-undef-init": 1,
        "no-undefined": 1,
        "no-unused-vars": 1,
        "no-invalid-this": 1,
        "comma-dangle": [
            1,
            "never"
        ],
        "no-extra-parens": [
            1,
            "all"
        ],
        "yoda": [
            1,
            "never",
            {
                "exceptRange": true
            }
        ],
        "no-unexpected-multiline": 1,
        "accessor-pairs": [
            1,
            {
                "getWithoutSet": true,
                "setWithoutGet": true
            }
        ],
        "complexity": [
            1,
            5
        ],
        "consistent-return": 1,
        "curly": [
            1,
            "all"
        ],
        "dot-notation": [
            1,
            {
                "allowKeywords": true
            }
        ],
        "array-bracket-spacing": [
            1,
            "never",
            {
                "singleValue": false,
                "objectsInArrays": false,
                "arraysInArrays": false
            }
        ],
        "brace-style": [
            1,
            "1tbs",
            {}
        ],
        "no-console": 0,
        "valid-jsdoc": 1,
        "block-scoped-var": 1,
        "default-case": 1,
        "dot-location": [
            1,
            "property"
        ],
        "eqeqeq": [
            1,
            "smart"
        ],
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-div-regex": 1,
        "no-else-return": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-floating-decimal": 1,
        "no-implicit-coercion": [
            1,
            {
                "boolean": true,
                "number": true,
                "string": true
            }
        ],
        "no-implied-eval": 1,
        "no-iterator": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-octal-escape": 1,
        "no-param-reassign": [
            1,
            {
                "props": true
            }
        ],
        "no-process-env": 1,
        "no-proto": 1,
        "no-return-assign": [
            1,
            "always"
        ],
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unused-expressions": 1,
        "no-useless-call": 1,
        "no-void": 1,
        "no-with": 1,
        "radix": 1,
        "vars-on-top": 1,
        "wrap-iife": [
            1,
            "inside"
        ],
        "strict": [
            1,
            "global"
        ],
        "init-declarations": [
            1,
            "always"
        ],
        "no-catch-shadow": 1,
        "no-label-var": 1,
        "no-shadow-restricted-names": 1,
        "no-shadow": [
            1,
            {
                "builtinGlobals": true,
                "hoist": "all"
            }
        ],
        "no-use-before-define": [
            1,
            "nofunc"
        ],
        "callback-return": 1,
        "no-mixed-requires": [
            1,
            true
        ],
        "no-new-require": 1,
        "no-path-concat": 1,
        "no-process-exit": 1,
        "no-restricted-modules": 1,
        "no-sync": 1,
        "arrow-parens": [
            1,
            "as-needed"
        ],
        "arrow-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "constructor-super": 1,
        "generator-star-spacing": 1,
        "no-class-assign": 1,
        "no-const-assign": 1,
        "no-this-before-super": 1,
        "no-var": 1,
        "object-shorthand": [
            1,
            "always"
        ],
        "prefer-const": 1,
        "prefer-spread": 1,
        "require-yield": 1,
        "semi": [
            1,
            "always"
        ],
        "keyword-spacing": 1,
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": [
            1,
            {
                "int32Hint": true
            }
        ],
        "space-unary-ops": [
            1,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "spaced-comment": [
            1,
            "always",
            {}
        ],
        "operator-linebreak": [
            1,
            "after"
        ],
        "handle-callback-err": [
            1,
            "err"
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "no-cond-assign": [
            1,
            "always"
        ],
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": [
            1,
            "both"
        ],
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 1,
        "no-fallthrough": 1,
        "no-octal": 1,
        "no-redeclare": 1,
        "no-delete-var": 1,
        "no-undef": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-mixed-spaces-and-tabs": [
            1,
            "smart-tabs"
        ],
        "quotes": [
            1,
            "single",
            "avoid-escape"
        ],
        "max-depth": [
            1,
            4
        ],
        "max-len": [
            1,
            120,
            4,
            {
                "ignoreComments": true,
                "ignoreUrls": true
            }
        ],
        "max-params": [
            1,
            5
        ],
        "max-statements": [
            1,
            20
        ],
        "no-bitwise": 1,
        "no-plusplus": 1,
        "comma-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last",
            {
                "exceptions": {
                    "ArrayExpression": false,
                    "ArrayPattern": false,
                    "ArrowFunctionExpression": false,
                    "CallExpression": false,
                    "FunctionDeclaration": false,
                    "FunctionExpression": false,
                    "ImportDeclaration": false,
                    "ObjectExpression": false,
                    "ObjectPattern": false,
                    "VariableDeclaration": false
                }
            }
        ],
        "computed-property-spacing": [
            1,
            "never"
        ],
        "consistent-this": 0,
        "eol-last": [
            1,
            "always"
        ]
    }
}
